==============================
VyOS 15
==============================
configure 
set interfaces ethernet eth0 address 220.0.3.2/24
set interfaces ethernet eth1 address 220.0.4.1/24
set interfaces ethernet eth2 address 220.0.9.1/24
set interfaces loopback lo address 15.15.15.15/32
set protocols ospf parameters router-id 15.15.15.15
set protocols ospf interface eth0 area 0.0.0.0
set protocols ospf interface eth1 area 0.0.0.0
set protocols ospf interface eth2 area 0.0.0.0
set protocols ospf interface lo area 0.0.0.0
set protocols bgp system-as 2500
set protocols bgp parameters router-id 15.15.15.15
set protocols bgp neighbor 17.17.17.17 remote-as 2500
set protocols bgp neighbor 18.18.18.18 remote-as 2500
set protocols bgp neighbor 19.19.19.19 remote-as 2500
set protocols bgp neighbor 20.20.20.20 remote-as 2500
set protocols bgp neighbor 17.17.17.17 address-family ipv4-unicast
set protocols bgp neighbor 18.18.18.18 address-family ipv4-unicast
set protocols bgp neighbor 19.19.19.19 address-family ipv4-unicast
set protocols bgp neighbor 20.20.20.20 address-family ipv4-unicast
set protocols bgp neighbor 17.17.17.17 update-source 15.15.15.15
set protocols bgp neighbor 18.18.18.18 update-source 15.15.15.15
set protocols bgp neighbor 19.19.19.19 update-source 15.15.15.15
set protocols bgp neighbor 20.20.20.20 update-source 15.15.15.15
set service dhcp-server shared-network-name LAN subnet 220.0.9.0/24 subnet-id 9
set service dhcp-server listen-interface eth2 
set service dhcp-server shared-network-name LAN option default-router 220.0.9.1 
set service dhcp-server shared-network-name LAN subnet 220.0.9.0/24 range clientes start 220.0.9.2 
set service dhcp-server shared-network-name LAN subnet 220.0.9.0/24 range clientes stop 220.0.9.252 
commit

