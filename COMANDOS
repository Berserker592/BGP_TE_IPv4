==============================
CISCO 1
==============================
enable
conf ter
hostname CISCO_1
inter lo0
ip addr 1.1.1.1 255.255.255.255
exit
inter g0/0
ip addr 210.0.2.2 255.255.255.0
no shut
exit
inter g0/1
ip addr 221.0.0.2 255.255.255.252
no shut
exit
inter g0/2
ip addr 221.0.0.14 255.255.255.252
no shut
exit
inter g0/3
ip addr 221.0.0.25 255.255.255.252
no shut
exit
inter g0/4
ip addr 221.0.0.45 255.255.255.252
no shut
exit
router eigrp 10
eigrp router-id 1.1.1.1
no auto-summary
network 210.0.2.0 0.0.0.255
network 1.1.1.1 0.0.0.0
exit
router bgp 1500
bgp router-id 1.1.1.1
neighbor 221.0.0.1 remote-as 200
neighbor 221.0.0.13 remote-as 300
neighbor 221.0.0.26 remote-as 400
neighbor 221.0.0.46 remote-as 500
neighbor GRUPO_FULLMESH peer-group 
neighbor GRUPO_FULLMESH remote-as 1500
neighbor GRUPO_FULLMESH update-source lo0
neighbor GRUPO_FULLMESH next-hop-self 
neighbor 2.2.2.2 peer-group GRUPO_FULLMESH
neighbor 3.3.3.3 peer-group GRUPO_FULLMESH
neighbor 4.4.4.4 peer-group GRUPO_FULLMESH
neighbor 5.5.5.5 peer-group GRUPO_FULLMESH
neighbor 6.6.6.6 peer-group GRUPO_FULLMESH
address-family ipv4 unicast
network 210.0.7.0 mask 255.255.255.0 
network 210.0.8.0 mask 255.255.255.0 
network 210.0.9.0 mask 255.255.255.0 
network 210.0.10.0 mask 255.255.255.0 
network 210.0.11.0 mask 255.255.255.0
network 210.0.12.0 mask 255.255.255.0 
exit
ip prefix-list SERVER27_30 seq 10 permit 220.0.5.0/24
ip prefix-list SERVER27_30 seq 20 permit 220.0.8.0/24

ip prefix-list SERVER28_31 seq 10 permit 220.0.6.0/24
ip prefix-list SERVER28_31 seq 20 permit 220.0.9.0/24

ip prefix-list SERVER29 permit 220.0.7.0/24

ip prefix-list INTERNET seq 1 permit 0.0.0.0/0 

route-map RUTA_SERVER27_30 permit 10 
set local-preference 150
match ip address prefix-list SERVER27_30
exit
route-map RUTA_SERVER28_31 permit 10 
set local-preference 160
match ip address prefix-list SERVER28_31
exit
route-map RUTA_SERVER29 permit 10 
set local-preference 170
match ip address prefix-list SERVER29
exit
route-map RUTA_INTERNET permit 5 
set local-preference 500
match ip address prefix-list INTERNET
exit

router bgp 1500
address-family ipv4 unicast
neighbor 221.0.0.1 route-map RUTA_SERVER27_30 in 
neighbor 221.0.0.13 route-map RUTA_SERVER28_31 in
neighbor 221.0.0.26 route-map RUTA_SERVER29 in
neighbor 221.0.0.46 route-map RUTA_INTERNET in
exit

