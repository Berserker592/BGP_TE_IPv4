set interfaces ethernet eth0 address 221.0.0.13/30
set interfaces ethernet eth1 address 221.0.0.17/30
set protocols bgp system-as 300
set protocols bgp parameters router-id 11.11.11.11
set protocols bgp neighbor 221.0.0.14 remote-as 1500
set protocols bgp neighbor 221.0.0.18 remote-as 302
set protocols bgp neighbor 221.0.0.14 address-family ipv4-unicast
set protocols bgp neighbor 221.0.0.18 address-family ipv4-unicast
commit
