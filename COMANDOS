ip addr 221.0.0.49 255.255.255.252
exit
interface GE5/0
ip addr 220.0.1.2 255.255.255.0
exit
interface GE6/0
ip addr 220.0.2.1 255.255.255.0
exit
ospf 10 router-id 17.17.17.17
exit
interface range G5/0 G6/0 lo0
ospf 10 area 0.0.0.0
exit
bgp 2500
router-id 17.17.17.17
peer 221.0.0.9 as-number 202
peer 221.0.0.21 as-number 302
peer 221.0.0.42 as-number 406
peer 221.0.0.50 as-number 500
group BGP_FULLMESH internal
peer BGP_FULLMESH as-number 2500
peer BGP_FULLMESH source-address 17.17.17.17
peer 18.18.18.18 group BGP_FULLMESH
peer 19.19.19.19 group BGP_FULLMESH
peer 15.15.15.15 group BGP_FULLMESH
peer 20.20.20.20 group BGP_FULLMESH
address-family ipv4 unicast 
peer 221.0.0.9 enable
peer 221.0.0.21 enable
peer 221.0.0.42 enable
peer 221.0.0.50 enable
network 220.0.5.0 255.255.255.0
network 220.0.6.0 255.255.255.0
network 220.0.7.0 255.255.255.0
network 220.0.8.0 255.255.255.0
network 220.0.9.0 255.255.255.0
network 0.0.0.0 0.0.0.0
peer BGP_FULLMESH ena
peer BGP_FULLMESH next-hop-local
exit

ip prefix-list SERVER_21_23 index 1 permit 210.0.7.0 24
ip prefix-list SERVER_21_23 index 2 permit 210.0.9.0 24
ip prefix-list SERVER_22_24 index 1 permit 210.0.8.0 24
ip prefix-list SERVER_22_24 index 2 permit 210.0.10.0 24
ip prefix-list SERVER_25_26 index 1 permit 210.0.11.0 24
ip prefix-list SERVER_25_26 index 2 permit 210.0.12.0 24
route-policy RUTA_SERVER_21_23 permit node 1
apply local-preference 180
if-match ip address prefix-list SERVER_21_23
exit
route-policy RUTA_SERVER_22_24 permit node 1
apply local-preference 190
if-match ip address prefix-list SERVER_22_24
exit
route-policy RUTA_SERVER_25_26 permit node 1
apply local-preference 200
if-match ip address prefix-list SERVER_25_26
exit

bgp 2500
address-family ipv4 unicast 
peer 221.0.0.9 route-policy RUTA_SERVER_21_23 import
peer 221.0.0.21 route-policy RUTA_SERVER_22_24 import
peer 221.0.0.42 route-policy RUTA_SERVER_25_26 import 
exit
